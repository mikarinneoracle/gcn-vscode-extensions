{
	"name": "gcn",
	"displayName": "Graal Cloud Native Tools",
	"description": "Graal Cloud Native tools for the VS Code",
	"version": "0.0.1",
	"preview": true,
	"license": "SEE LICENSE IN LICENSE.txt",
	"publisher": "oracle-labs-graalvm",
	"author": {
		"name": "Oracle Labs"
	},
	"icon": "images/graalvm-cloud-logo.png",
	"homepage": "https://github.com/graalvm/vscode-extensions/blob/master/gcn/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/graalvm/vscode-extensions.git"
	},
	"bugs": {
		"url": "https://github.com/graalvm/vscode-extensions/issues"
	},
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"keywords": [
		"java",
		"cloud",
		"native image"
	],
	"activationEvents": [
		"onCommand:gcn.showWelcomePage",
		"onCommand:gcn.oci.projectAudit.execute",
		"onCommand:gcn.createGcnProject",
		"onCommand:gcn.oci.initializeSshKeys",
		"onView:gcn-services"
	],
	"main": "./dist/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Graal Cloud Native",
			"properties": {
				"gcn.showWelcomePage": {
					"type": "boolean",
					"default": true,
					"description": "Show Graal Cloud Native page on extension activation"
				},
				"gcn.home": {
					"type": "string",
					"default": "",
					"description": "Optional path to Graal Cloud CLI installation",
					"scope": "machine"
				}
			}
		},
		"commands": [
			{
				"command": "gcn.oci.undeployFromCloud",
				"title": "Undeploy Opened Folders",
				"category": "GCN"
			},
			{
				"command": "gcn.showWelcomePage",
				"title": "Show Graal Cloud Native Page",
				"category": "GCN"
			},
			{
				"command": "gcn.deployToCloud",
				"title": "Deploy to Cloud",
				"icon": "$(cloud-upload)",
				"category": "GCN"
			},
			{
				"command": "gcn.addContent",
				"title": "Add Content...",
				"icon": "$(add)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.openInBrowser",
				"title": "Open in Browser",
				"icon": "$(link-external)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.openInConsole",
				"title": "Open in Cloud Console",
				"icon": "$(globe)",
				"category": "GCN"
			},
			{
				"command": "gcn.renameNode",
				"title": "Rename",
				"icon": "$(edit)",
				"category": "GCN"
			},
			{
				"command": "gcn.removeNode",
				"title": "Remove",
				"icon": "$(remove-close)",
				"category": "GCN"
			},
			{
				"command": "gcn.showReport",
				"title": "Show Report",
				"icon": "$(output)",
				"category": "GCN"
			},
			{
				"command": "gcn.reloadNode",
				"title": "Reload",
				"icon": "$(refresh)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.runBuildPipeline",
				"title": "Run Build Pipeline",
				"icon": "$(run)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.runDeployPipeline",
				"title": "Run Deployment Pipeline",
				"icon": "$(run)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.getBuildArtifact",
				"title": "Get Build Artifact(s)",
				"icon": "$(cloud-download)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.downloadGenericArtifact",
				"title": "Download Artifact",
				"icon": "$(cloud-download)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.downloadLatestGenericArtifact",
				"title": "Download Latest Artifact",
				"icon": "$(cloud-download)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.pullDockerImage",
				"title": "Pull Docker Image",
				"icon": "$(cloud-download)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.pullLatestDockerImage",
				"title": "Pull Latest Docker Image",
				"icon": "$(cloud-download)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.projectAudit.execute",
				"title": "Run Project Audit",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.initializeSshKeys",
				"title": "Initialize SSH Keys",
				"category": "GCN"
			},
			{
				"command": "gcn.createGcnProject",
				"title": "Create GCN Project",
				"category": "GCN"
			}
		],
		"viewsWelcome": [
			{
				"view": "gcn-services",
				"contents": "Initializing Graal Cloud Native Services...",
				"when": "!gcn.servicesInitialized"
			},
			{
				"view": "gcn-services",
				"contents": "Click the Import From Cloud button to clone an existing cloud folder locally:\n[Import From Cloud](command:gcn.importFromCloud)\n\nTo learn more about the Graal Cloud Native framework [read the docs](https://developer.oracle.com/java/graalvm/cloud-native/).",
				"when": "gcn.servicesInitialized && workspaceFolderCount == 0"
			},
			{
				"view": "gcn-services",
				"contents": "Click the Deploy to Cloud button to deploy the current folder(s) to cloud:\n[Deploy to Cloud](command:gcn.deployToCloud)\n\nTo learn more about the Graal Cloud Native framework [read the docs](https://developer.oracle.com/java/graalvm/cloud-native/).",
				"when": "gcn.servicesInitialized && workspaceFolderCount > 0 && gcn.serviceFoldersCount == 0"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "gcn-services",
					"name": "GCN Services",
					"icon": "resources/GCN_logo.png"
				}
			]
		},
		"menus": {
			"commandPalette": [
				{
					"command": "gcn.deployToCloud",
					"when": "false"
				},
				{
					"command": "gcn.addContent",
					"when": "false"
				},
				{
					"command": "gcn.oci.openInBrowser",
					"when": "false"
				},
				{
					"command": "gcn.oci.openInConsole",
					"when": "false"
				},
				{
					"command": "gcn.renameNode",
					"when": "false"
				},
				{
					"command": "gcn.removeNode",
					"when": "false"
				},
				{
					"command": "gcn.showReport",
					"when": "false"
				},
				{
					"command": "gcn.reloadNode",
					"when": "false"
				},
				{
					"command": "gcn.oci.pullDockerImage",
					"when": "false"
				},
				{
					"command": "gcn.oci.pullLatestDockerImage",
					"when": "false"
				},
				{
					"command": "gcn.oci.runBuildPipeline",
					"when": "false"
				},
				{
					"command": "gcn.oci.runDeployPipeline",
					"when": "false"
				},
				{
					"command": "gcn.oci.getBuildArtifact",
					"when": "false"
				},
				{
					"command": "gcn.oci.downloadGenericArtifact",
					"when": "false"
				},
				{
					"command": "gcn.oci.downloadLatestGenericArtifact",
					"when": "false"
				}
			],
			"editor/context": [],
			"view/title": [
				{
					"command": "gcn.deployToCloud",
					"when": "view == gcn-services && gcn.servicesInitialized && gcn.globalDeployAction && nbJavaLSReady",
					"group": "navigation@1"
				},
				{
					"command": "gcn.addContent",
					"when": "view == gcn-services && gcn.servicesInitialized && workspaceFolderCount == 1 && gcn.serviceFoldersCount == 1",
					"group": "navigation@1"
				}
			],
			"view/item/context": [
				{
					"command": "gcn.deployToCloud",
					"when": "viewItem in gcn.deployToCloudNodes && nbJavaLSReady",
					"group": "inline@1"
				},
				{
					"command": "gcn.addContent",
					"when": "viewItem in gcn.addContentNodes",
					"group": "inline@1"
				},
				{
					"command": "gcn.oci.openInBrowser",
					"when": "viewItem == gcn.oci.deploymentPipelineNode-deployments-available",
					"group": "inline@2"
				},
				{
					"command": "gcn.oci.openInConsole",
					"when": "viewItem in gcn.oci.openInConsoleNodes",
					"group": "a_ociContextMenu@1"
				},
				{
					"command": "gcn.renameNode",
					"when": "viewItem in gcn.renameableNodes",
					"group": "contextMenu@1"
				},
				{
					"command": "gcn.removeNode",
					"when": "viewItem in gcn.removableNodes",
					"group": "contextMenu@2"
				},
				{
					"command": "gcn.showReport",
					"when": "viewItem in gcn.showReportNodes",
					"group": "inline@2"
				},
				{
					"command": "gcn.reloadNode",
					"when": "viewItem in gcn.reloadableNodes",
					"group": "inline@3"
				},
				{
					"command": "gcn.oci.runBuildPipeline",
					"when": "viewItem == gcn.oci.buildPipelineNode || viewItem == gcn.oci.buildPipelineNode-artifacts-available",
					"group": "inline@1"
				},
				{
					"command": "gcn.oci.runDeployPipeline",
					"when": "viewItem == gcn.oci.deploymentPipelineNode || viewItem == gcn.oci.deploymentPipelineNode-deployments-available",
					"group": "inline@1"
				},
				{
					"command": "gcn.oci.getBuildArtifact",
					"when": "viewItem == gcn.oci.buildPipelineNode-artifacts-available",
					"group": "inline@2"
				},
				{
					"command": "gcn.oci.downloadGenericArtifact",
					"when": "viewItem == gcn.oci.genericArtifactNode",
					"group": "inline@2"
				},
				{
					"command": "gcn.oci.downloadLatestGenericArtifact",
					"when": "viewItem == gcn.oci.genericDeployArtifactNode",
					"group": "inline@2"
				},
				{
					"command": "gcn.oci.pullDockerImage",
					"when": "viewItem == gcn.oci.containerImageNode",
					"group": "inline@2"
				},
				{
					"command": "gcn.oci.pullLatestDockerImage",
					"when": "viewItem == gcn.oci.ocirDeployArtifactNode",
					"group": "inline@2"
				},
				{
					"command": "gcn.oci.projectAudit.execute",
					"when": "view == javaProjectExplorer && viewItem =~ /java:project(?=.*?\\b\\+java\\b)(?=.*?\\b\\+uri\\b)/"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "NODE_OPTIONS='--max-old-space-size=8192' webpack --mode production",
		"compile": "webpack --mode development",
		"info": "webpack --display-modules",
		"watch": "webpack --mode development --watch",
		"test-compile": "tsc -p ./",
		"lint": "tslint -p ./"
	},
	"dependencies": {
		"mustache": "4.0.1",
		"oci-sdk": "^2.28.0",
		"vscode-kubernetes-tools-api": "^1.3.0",
		"which": "^2.0.2"
	},
	"devDependencies": {
		"@types/mustache": "^4.0.1",
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.64.0",
		"@types/which": "^2.0.1",
		"@vscode/codicons": "0.0.20",
		"ts-loader": "^9.3.1",
		"tslint": "^5.12.1",
		"typescript": "^4.7.2",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	},
    "extensionDependencies": [
		"asf.apache-netbeans-java",
		"ms-kubernetes-tools.vscode-kubernetes-tools"
    ]
}
