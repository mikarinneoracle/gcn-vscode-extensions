{
	"name": "graalvm-gcn",
	"displayName": "Graal Cloud Native Tools",
	"description": "Graal Cloud Native tools for the VS Code",
	"version": "0.0.1",
	"preview": true,
	"license": "SEE LICENSE IN LICENSE.txt",
	"publisher": "oracle-labs-graalvm",
	"author": {
		"name": "Oracle Labs"
	},
	"icon": "images/graalvm-cloud-logo.png",
	"homepage": "https://github.com/graalvm/vscode-extensions/blob/master/gcn/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/graalvm/vscode-extensions.git"
	},
	"bugs": {
		"url": "https://github.com/graalvm/vscode-extensions/issues"
	},
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"keywords": [
		"java",
		"cloud",
		"native image"
	],
	"activationEvents": [
		"onView:gcn-services"
	],
	"main": "./dist/extension",
	"contributes": {
		"commands": [
			{
				"command": "gcn.oci.undeployFromCloud",
				"title": "Undeploy Opened Folders",
				"category": "GCN"
			},
			{
				"command": "gcn.addContent",
				"title": "Add Content...",
				"icon": "$(add)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.openInConsole",
				"title": "Open in Cloud Console",
				"icon": "$(globe)",
				"category": "GCN"
			},
			{
				"command": "gcn.renameNode",
				"title": "Rename",
				"icon": "$(edit)",
				"category": "GCN"
			},
			{
				"command": "gcn.removeNode",
				"title": "Remove",
				"icon": "$(remove-close)",
				"category": "GCN"
			},
			{
				"command": "gcn.showReport",
				"title": "Show Report",
				"icon": "$(output)",
				"category": "GCN"
			},
			{
				"command": "gcn.reloadNode",
				"title": "Reload",
				"icon": "$(refresh)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.runBuildPipeline",
				"title": "Run Build Pipeline",
				"icon": "$(run)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.downloadArtifact",
				"title": "Download Artifact",
				"icon": "$(cloud-download)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.pullDockerImage",
				"title": "Pull Image",
				"icon": "$(cloud-download)",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.projectAudit.execute",
				"title": "Run Project Audit",
				"category": "GCN"
			},
			{
				"command": "gcn.oci.initializeSshKeys",
				"title": "Initialize SSH Keys",
				"category": "GCN"
			}
		],
		"viewsWelcome": [
			{
				"view": "gcn-services",
				"contents": "Initializing Graal Cloud Native Services...",
				"when": "!gcn.servicesInitialized"
			},
			{
				"view": "gcn-services",
				"contents": "Click the Import DevOps Project button to clone an existing DevOps Project locally:\n[Import DevOps Project](command:gcn.oci.importFromCloud)\n\nTo learn more about the Graal Cloud Native framework [read the docs](https://developer.oracle.com/java/graalvm/cloud-native/).",
				"when": "gcn.servicesInitialized && workspaceFolderCount == 0"
			},
			{
				"view": "gcn-services",
				"contents": "Click the Deploy to Cloud button to deploy the current folder(s) to the cloud:\n[Deploy to Cloud](command:gcn.oci.deployToCloud)\n\nTo learn more about the Graal Cloud Native framework [read the docs](https://developer.oracle.com/java/graalvm/cloud-native/).",
				"when": "gcn.servicesInitialized && workspaceFolderCount > 0 && gcn.serviceFoldersCount == 0"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "gcn-services",
					"name": "GCN Services",
					"icon": "resources/GCN_logo.png"
				}
			]
		},
		"menus": {
			"commandPalette": [
				{
					"command": "gcn.addContent",
					"when": "false"
				},
				{
					"command": "gcn.oci.openInConsole",
					"when": "false"
				},
				{
					"command": "gcn.renameNode",
					"when": "false"
				},
				{
					"command": "gcn.removeNode",
					"when": "false"
				},
				{
					"command": "gcn.showReport",
					"when": "false"
				},
				{
					"command": "gcn.reloadNode",
					"when": "false"
				},
				{
					"command": "gcn.oci.pullDockerImage",
					"when": "false"
				},
				{
					"command": "gcn.oci.runBuildPipeline",
					"when": "false"
				},
				{
					"command": "gcn.oci.downloadArtifact",
					"when": "false"
				}
			],
			"editor/context": [],
			"view/title": [
				{
					"command": "gcn.addContent",
					"when": "view == gcn-services && gcn.servicesInitialized && gcn.serviceFoldersCount == 1",
					"group": "navigation@1"
				}
			],
			"view/item/context": [
				{
					"command": "gcn.addContent",
					"when": "viewItem in gcn.addContentNodes",
					"group": "inline@1"
				},
				{
					"command": "gcn.oci.openInConsole",
					"when": "viewItem in gcn.oci.openInConsoleNodes",
					"group": "a_ociContextMenu@1"
				},
				{
					"command": "gcn.renameNode",
					"when": "viewItem in gcn.renameableNodes",
					"group": "contextMenu@1"
				},
				{
					"command": "gcn.removeNode",
					"when": "viewItem in gcn.removableNodes",
					"group": "contextMenu@2"
				},
				{
					"command": "gcn.showReport",
					"when": "viewItem in gcn.showReportNodes",
					"group": "inline@2"
				},
				{
					"command": "gcn.reloadNode",
					"when": "viewItem in gcn.reloadableNodes",
					"group": "inline@3"
				},
				{
					"command": "gcn.oci.runBuildPipeline",
					"when": "viewItem == gcn.oci.buildPipelineNode || viewItem == gcn.oci.buildPipelineNode-artifacts-available",
					"group": "inline@1"
				},
				{
					"command": "gcn.oci.downloadArtifact",
					"when": "viewItem == gcn.oci.buildPipelineNode-artifacts-available",
					"group": "inline@2"
				},
				{
					"command": "gcn.oci.pullDockerImage",
					"when": "viewItem == gcn.oci.containerImageNode",
					"group": "inline@2"
				},
				{
					"command": "gcn.oci.projectAudit.execute",
					"when": "view == javaProjectExplorer && viewItem =~ /java:project(?=.*?\\b\\+java\\b)(?=.*?\\b\\+uri\\b)/"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode development",
		"info": "webpack --display-modules",
		"watch": "webpack --mode development --watch",
		"test-compile": "tsc -p ./",
		"lint": "tslint -p ./"
	},
	"dependencies": {
		"oci-sdk": "^2.28.0",
		"vscode-kubernetes-tools-api": "^1.0.0"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.49.0",
		"@vscode/codicons": "0.0.20",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"ts-loader": "^6.2.1",
		"webpack": "^5.40.0",
		"webpack-cli": "^4.7.2"
	}
}
